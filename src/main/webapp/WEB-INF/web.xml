<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >


<web-app>
  <servlet>
    <servlet-name>MyServlet</servlet-name>
    <servlet-class>com.bjpowernod.controller.MyServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>MyServlet</servlet-name>
    <url-pattern>/student/insert</url-pattern>
    <url-pattern>/student/delete</url-pattern>
    <url-pattern>/student/update</url-pattern>
    <url-pattern>/student/select</url-pattern>
  </servlet-mapping>
</web-app>
